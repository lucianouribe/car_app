<h1>Dealerships#new</h1>
<p>Find me in app/views/dealerships/new.html.erb</p>

<%= form_for @dealership do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :amount_of_cars %>
  <%= f.number_field :amount_of_cars %>
  <%= f.label :second_hand %>
  <%= f.select :second_hand, options_for_select([true, false])%>
  <%= f.submit %>
<% end %>

<%= link_to 'back', :back %>
